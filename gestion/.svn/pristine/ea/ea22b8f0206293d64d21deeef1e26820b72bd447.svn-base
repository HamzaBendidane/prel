/**
 * Implements hook_menu_default_menu_custom().
 */
function ecofolio_menus_menu_default_menu_custom() {
  $menus = array();

  // Exported menu: main-menu.
  $menus['main-menu'] = array(
    'menu_name' => 'main-menu',
    'title' => 'Main menu',
    'description' => 'The <em>Main</em> menu is used on many sites to show the major sections of the site, often in a top navigation bar.',
  );
  // Exported menu: menu-footer-menu.
  $menus['menu-footer-menu'] = array(
    'menu_name' => 'menu-footer-menu',
    'title' => 'Footer',
    'description' => '',
  );
  // Exported menu: menu-sidebar-main-menu.
  $menus['menu-sidebar-main-menu'] = array(
    'menu_name' => 'menu-sidebar-main-menu',
    'title' => 'Sidebar main menu',
    'description' => '',
  );
  // Exported menu: menu-sidebar-top-menu.
  $menus['menu-sidebar-top-menu'] = array(
    'menu_name' => 'menu-sidebar-top-menu',
    'title' => 'Sidebar top menu',
    'description' => '',
  );
  // Exported menu: menu-top-menu.
  $menus['menu-top-menu'] = array(
    'menu_name' => 'menu-top-menu',
    'title' => 'Top menu',
    'description' => 'Menu on the top header zone',
  );
  // Translatables
  // Included for use with string extractors like potx.
  t('Footer');
  t('Main menu');
  t('Menu on the top header zone');
  t('Sidebar main menu');
  t('Sidebar top menu');
  t('The <em>Main</em> menu is used on many sites to show the major sections of the site, often in a top navigation bar.');
  t('Top menu');


  return $menus;
}
