/**
 * Implements hook_context_default_contexts().
 */
function ecofolio_homeunivers_context_default_contexts() {
  $export = array();

  $context = new stdClass();
  $context->disabled = FALSE; /* Edit this to true to make a default context disabled initially */
  $context->api_version = 3;
  $context->name = 'homeunivers';
  $context->description = '';
  $context->tag = '';
  $context->conditions = array(
    'node' => array(
      'values' => array(
        'home_univers' => 'home_univers',
      ),
      'options' => array(
        'node_form' => '1',
      ),
    ),
  );
  $context->reactions = array(
    'delta' => array(
      'delta_template' => 'delta_homeunivers',
    ),
  );
  $context->condition_mode = 0;
  $export['homeunivers'] = $context;

  return $export;
}
