/**
 * Implements hook_default_fe_block_boxes().
 */
function ecofolio_custom_blocks_default_fe_block_boxes() {
  $export = array();

  $fe_block_boxes = new stdClass();
  $fe_block_boxes->info = 'Copyright Footer';
  $fe_block_boxes->format = 'full_html';
  $fe_block_boxes->machine_name = 'copyright';
  $fe_block_boxes->body = '&copy; Copyright 2014. Ecofolio, tous droits réservés';

  $export['copyright'] = $fe_block_boxes;

  $fe_block_boxes = new stdClass();
  $fe_block_boxes->info = 'Logo principal';
  $fe_block_boxes->format = 'full_html';
  $fe_block_boxes->machine_name = 'logo';
  $fe_block_boxes->body = '<a href="/"><img alt="Logo Ecofolio" id="logo" src="/sites/all/themes/ecofolio/images/logo.png" style="float:left; height:85px; width:289px" /></a>';

  $export['logo'] = $fe_block_boxes;

  return $export;
}
