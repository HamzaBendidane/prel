/*--------------------------------------------
 * checkbox and radio
 *--------------------------------------------*/
li:not(#foo) > fieldset > div > span > input[type='checkbox'] {
    opacity: 0;
    float: left;
    width: 18px;
}

div:not(#foo) > input[type='checkbox'] + label {
    margin: 0;
    clear: none;
    padding: 2px 0 0px 24px;
    cursor: pointer;
    background: url(../images/check_off.png) left top no-repeat;
}

div:not(#foo) > input[type='checkbox']:checked + label {
    background-image: url(../images/check_on.png);
}

div:not(#foo) > input[type='checkbox'] {
    display : none;
}

div:not(#foo) > input[type='radio'] + label {
    margin: 0;
    clear: none;
    padding: 2px 0 2px 24px;
    cursor: pointer;
    background: url(../images/radio_off.png) left center no-repeat;
    display: inline-block;
}

div:not(#foo) > input[type='radio']:checked + label {
    background-image: url(../images/radio_on.png);
}

div:not(#foo) > .form-item.form-type-radio {
	overflow : hidden;
}

div:not(#foo) > input[type='radio'] {
    margin-left: -13px;
    width: 0px;
}

div:not(#foo) > .form-item.form-type-radio label {
	margin-left: 10px;
}

.form-item.form-type-radio {
    display : block;
}

body .form-checkboxes .form-item, body .form-radios .form-item  {
    margin : 0;
}


form .form-item.form-type-radios > label {
    display: block;
    width: 100%;
}

