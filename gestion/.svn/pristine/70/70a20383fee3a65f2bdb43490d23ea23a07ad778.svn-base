/*--------------------------------------------
 * Breadcrumb
 *--------------------------------------------*/

#steps {
    position: relative;
    border-top: 1px solid #196681;
}

#steps .selector-bar {
    background: url('../images/headerfood.png') 125px 0;
    height: 24px;
    width: 100%;
    left: 0;
    margin-top: 4px;
}

.selector-bar.bottom {
    background: url('../images/headerfood.png') 125px 0;
    height: 30px;
    width: 100%;
    left: 0;
}


#steps .step {
    float: left;
    color: #196681;
    text-align: center;
    font-size: 12px;
    padding: 0;
    min-width: 80px;
    margin-top: 10px;
    width: 12.50%;
}

#steps .step.active {
    color: #d54015;
    font-weight: bold;
}


#steps .active .selector-bar {
    background: url('../images/headerarrowfood.png') center 0 no-repeat;
    background-size: 100% 100%;
    height: 24px;
    width: 100%;
    left: 0;
}


#steps .step .text {
    text-transform: uppercase;
    font-weight: 400;
}

#steps .step.active .text {
    font-weight: 600;
}

#steps .step .number-container {
    font-weight: 600;
    height: 22px;
    width: 22px;
    line-height: 22px;
    display: inline-block;
    margin-top: 5px;
    background: url('../images/step-round.png');
}

