<script type="text/javascript">
var is_show_form=0;
function hide_form() {
	if (!ie7) this.document.getElementById('popup_overlay').style.display="none";
	this.document.getElementById('frame_form').style.display="none";
	is_show_form=0;
}
function show_form(ftype) {
	if (!ie7) this.document.getElementById('popup_overlay').style.display="block";
	this.document.getElementById('frame_form').style.display="block";
	is_show_form=1;
	set_lead_source(ftype);
}
function toggle_form() {
	if (is_show_form==0) { show_form() } else { hide_form() }
}
function set_lead_source(ftype) {
	var url = window.location.toString();
	if (url.lastIndexOf('#form_')>0) {
		var ftype=url.split('#form_');
		document.getElementById('lead_source').value = "saatchione_"+ftype[1];
		if (ftype[1]=="adhesion") {
			document.getElementById('form_title').innerHTML = "Formulaire d'Adh&eacute;sion";
		}
		else {
			document.getElementById('form_title').innerHTML = "Formulaire de Contact";
		}
		//alert("DEBUG ::: lead_source="+document.getElementById('lead_source').value);
	}
}
function page_init() {
	var url = window.location.toString();
	if (url.lastIndexOf('#form_')>0) { show_form(); } else { hide_form(); }
}
</script>
