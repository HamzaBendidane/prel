/**
 * Implements hook_ctools_plugin_api().
 */
function ecofolio_annonces_ctools_plugin_api($module = NULL, $api = NULL) {
  if ($module == "field_group" && $api == "field_group") {
    return array("version" => "1");
  }
  if ($module == "strongarm" && $api == "strongarm") {
    return array("version" => "1");
  }
}

/**
 * Implements hook_views_api().
 */
function ecofolio_annonces_views_api($module = NULL, $api = NULL) {
  return array("api" => "3.0");
}

/**
 * Implements hook_node_info().
 */
function ecofolio_annonces_node_info() {
  $items = array(
    'annonces' => array(
      'name' => t('Annonces'),
      'base' => 'node_content',
      'description' => '',
      'has_title' => '1',
      'title_label' => t('Annonce'),
      'help' => '',
    ),
  );
  return $items;
}
