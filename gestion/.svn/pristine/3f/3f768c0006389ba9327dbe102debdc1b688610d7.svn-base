	  /* rempli un cookie avec l'id des bloc rem déja cliqués */ 
	  $('.rem-custom-link').click(function(){
	    var nid = $(this).attr('id').replace('rem-', '');
	    var cookieRule = getCookie('rulesMatched');
	    console.log(cookieRule);
	    if(cookieRule == "") {
	      document.cookie="rulesMatched="+ nid +"; path=/";
	    } else {
	      document.cookie="rulesMatched="+ cookieRule + "|" + nid +"; path=/";
	    }
	  });
}(jQuery));

function getCookie(cname) {
  var name = cname + "=";
  var ca = document.cookie.split(';');
  for(var i=0; i<ca.length; i++) {
      var c = ca[i];
      while (c.charAt(0)==' ') c = c.substring(1);
      if (c.indexOf(name) != -1) return c.substring(name.length,c.length);
  }
  return "";
} 