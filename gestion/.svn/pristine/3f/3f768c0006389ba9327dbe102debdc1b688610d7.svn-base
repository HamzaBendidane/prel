(function ($) {    
	$(document).ready(function(){     
	  /* gestion sous menu collectivité */ 
	  /* sidebar maenu */
//	  if($('.sidebar-homepage.active-trail').length == 0) {
//	    $('.sidebar-homepage').css('display', 'block');
